package aula03.salaaula;

public class TestaQuadrado {

	public static void main(String[] args) {
		Quadrado q1 = new Quadrado();
		Quadrado q2 = new Quadrado();
		Quadrado q3 = new Quadrado();
		
		
		q1.cor = "verde";
		q2.cor = "Vermelho";
		
		System.out.println("Cor quadrado 1: "+ q1.cor);
		System.out.println("Cor do quadrado 1: "+ q2.cor);
		System.out.println("Cor do quadrado 1: "+ q3.cor);
		System.out.println("Cor da classe quadrado: "+ Quadrado.cor);

	}

}
